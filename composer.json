{
    "name": "021/ethereum-oracles",
    "description": "Provide ethereum oracles apis",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "O21\\EthereumOracles\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "guzzlehttp/guzzle": "^7.4",
        "php": "^8.1",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit tests"
        ]
    }
}
